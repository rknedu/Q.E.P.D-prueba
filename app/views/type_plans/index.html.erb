<p id="notice"><%= notice %></p>

<h1>Type Plans</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_plans.each do |type_plan| %>
      <tr>
        <td><%= type_plan.nombre %></td>
        <td><%= link_to 'Show', type_plan %></td>
        <td><%= link_to 'Edit', edit_type_plan_path(type_plan) %></td>
        <td><%= link_to 'Destroy', type_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Plan', new_type_plan_path %>
