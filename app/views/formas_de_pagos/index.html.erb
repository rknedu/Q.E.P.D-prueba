<p id="notice"><%= notice %></p>

<h1>Formas De Pagos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formas_de_pagos.each do |formas_de_pago| %>
      <tr>
        <td><%= formas_de_pago.nombre %></td>
        <td><%= link_to 'Show', formas_de_pago %></td>
        <td><%= link_to 'Edit', edit_formas_de_pago_path(formas_de_pago) %></td>
        <td><%= link_to 'Destroy', formas_de_pago, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formas De Pago', new_formas_de_pago_path %>
